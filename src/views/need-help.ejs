<!-- Need Help Page-->
<div class="hidden slide" id="lslf__v_need-help">
    <section class="content text-center mb-3">
        <h4 class="text-system-black">
            <span
                id="lslf__need-help_headline"
                data-content="%%Custom Headline%%"
            ></span>
        </h4>
        <p>
            <small>
                <span
                    id="lslf__need-help_message"
                    data-content="%%Custom message configured in WebServices can be long or short and, but CSS text replaceable will not supports rich text like bold fonts and web links to external sites.%%*"
                ></span>
            </small>
        </p>
        <a
            type="button"
            class="btn btn-primary w-100 mt-2 link-button"
            id="lslf__need-help_btn_custom-cta"
        >
            <span
                id="lslf__need-help_btn_custom-cta_label"
                class="label"
                data-content="Custom CTA"
            >
            </span>
        </a>
        <button
            type="button"
            class="btn link text-primary slide-switcher text-center mt-4"
            data-go-to="lslf__v_login"
            id="lslf__need-help_btn_back"
        >
            <small>
                <span
                    id="lslf__need-help_btn_back_label"
                    class="label"
                    data-content="Return to Sign In"
                >
                </span>
            </small>
        </button>
    </section>
</div>
